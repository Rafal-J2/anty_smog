class MarkerHelper {
  // ...

  Marker buildMarker(Map<String, dynamic> station) {
    num pm25Avg = station['data']['pm25_avg'] as num;

    return Marker(
      // ...
      icon: pm25Avg > 55
          ? _iconLoader.markerIcon4
          : pm25Avg > 35
          ? _iconLoader.markerIcon3
          : pm25Avg > 13
          ? _iconLoader.markerIcon2
          : _iconLoader.markerIcon,
    );
  }
}

